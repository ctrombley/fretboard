{
  "name": "fretboard",
  "version": "1.0.0",
  "description": "it's a fretboard",
  "scripts": {
    "build": "webpack --config webpack.production.config.js --progress --profile --colors",
    "start": "node src/server",
    "dev": "webpack-dashboard -- webpack-dev-server -d --progress --profile --colors",
    "lint": "eslint *.jsx *.js src/**/*.js src/**/*.jsx spec/**/*.js",
    "test": "mocha --compilers js:babel-core/register  spec/**/*",
    "watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ctrombley/fretboard.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "reload",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Chris Trombley <ctrombley@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^3.5.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^5.0.2",
    "eslint-plugin-react": "^7.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^6.0.6",
    "sockjs-client": "^1.1.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "express": "^4.15.3",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2"
