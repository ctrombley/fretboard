{
  "name": "fretboard",
  "version": "1.0.0",
  "description": "it's a fretboard",
  "scripts": {
    "build": "webpack --config webpack.production.config.js --progress --profile --colors",
    "dev": "webpack-dashboard -- webpack-dev-server -d --progress --profile --colors",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "start": "webpack-dev-server -d --progress --profile --colors",
    "test": "mocha --compilers js:babel-core/register  spec/**/*",
    "watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ctrombley/fretboard.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "reload",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Chris Trombley <ctrombley@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-loader": "6.0.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "chai": "^3.5.0",
    "css-loader": "0.23.1",
    "eslint": "2.7.0",
    "eslint-plugin-react": "5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.2.0",
    "node-sass": "3.8.0",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "4.0.0",
    "sockjs-client": "^1.1.1",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-cleanup-plugin": "^0.4.2",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.4.2",
    "react-dom": "15.4.2"
  }
}
